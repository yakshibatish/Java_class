public class NumberPattern {
    public static void main(String[] args) {
        int rows = 4;
        int cols = 5;

        for (int i = 1; i <= rows; i++) {        // rows
            for (int j = 1; j <= cols; j++) {    // numbers in each row
                System.out.print(j + " ");
            }
            System.out.println();                // move to next line
        }
    }
}
